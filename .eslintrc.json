{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "extends": ["prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error", {"semi": false, "singleQuote": true}],
      "arrow-body-style": "off",
      "prefer-arrow-callback": "off"
    }
}
